    "import sqlite3\n",
    "import networkx as nx\n",
    "conn=sqlite3.connect('xqfriends_0504')\n",
    "cur=conn.cursor()\n",
    "def graph_from_data():\n",
    "    cur.execute('select * from Follow@')\n",
    "    g=nx.DiGraph(name='xq social network')\n",
    "    for i in cur.fetchall():\n",
    "        g.add_edge(i[0],i[1])\n",
    "    return g"
   ]
